{
  "name": "post",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:aot": "ng serve --aot",
    "start:prod": "ng serve --prod",
    "start:post": "ng serve --configuration post",
    "start:rest": "json-server --watch mock/db.json",
